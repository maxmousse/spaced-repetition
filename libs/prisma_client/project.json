{
  "name": "prisma_client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/prisma_client/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:check",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/prisma_client"
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/prisma_client"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/prisma_client"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["cargo prisma-generate-rust-client"],
        "cwd": "libs/prisma_client"
      },
      "inputs": ["{projectRoot}/prisma/schema.prisma"],
      "outputs": ["{projectRoot}/src/prisma.rs"]
    }
  },
  "tags": []
}
